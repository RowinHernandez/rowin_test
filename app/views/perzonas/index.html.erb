<h1>Listing perzonas</h1>

<table>
  <tr>
    <th>Cedula</th>
    <th>Direccion</th>
    <th>Nombre</th>
    <th>Apellido</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @perzonas.each do |perzona| %>
  <tr>
    <td><%= perzona.cedula %></td>
    <td><%= perzona.direccion %></td>
    <td><%= perzona.nombre %></td>
    <td><%= perzona.apellido %></td>
    <td><%= link_to 'Show', perzona %></td>
    <td><%= link_to 'Edit', edit_perzona_path(perzona) %></td>
    <td><%= link_to 'Destroy', perzona, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Perzona', new_perzona_path %>
